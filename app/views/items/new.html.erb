<h1>新規投稿画面</h1>
<%= form_with model: @item, url: items_path, local: true do |f| %>
  <%= f.label :"タイトル" %>
  <%= f.text_field :title %>
  <%= f.label :"作品画像" %>
  <%= f.attachment_field :item_images_images, multiple: true %>
  <%= f.label :"説明文" %>
  <%= f.text_area :body %>
  <%= f.submit "投稿"%>
<% end %>